{
    "name": "monet/auth",
    "description": "The auth component for the Monet framework.",
    "keywords": [
        "monet-cms",
        "laravel",
        "auth"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.3",
        "monet/support": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Monet\\Auth\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Monet\\Framework\\Auth\\AuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
